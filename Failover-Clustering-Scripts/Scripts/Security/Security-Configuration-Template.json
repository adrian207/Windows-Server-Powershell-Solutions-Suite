    "AuditLogging": {
        "Enabled": true,
        "AuditLevel": "Comprehensive",
        "IncludeSecurityEvents": true,
        "IncludeConfigurationChanges": true,
        "IncludeResourceChanges": true,
        "IncludeNetworkEvents": true,
        "LogRetentionDays": 365,
        "LogLocation": "C:\\ClusterLogs\\Audit",
        "LogRotation": true,
        "CompressionEnabled": true,
        "EncryptionEnabled": true
    },
    "CertificateConfiguration": {
        "CertificateType": "Cluster",
        "CertificateStore": "LocalMachine",
        "CertificateSubject": "CN=Cluster-Cert",
        "CertificateValidityDays": 365,
        "AutoRenewal": true,
        "KeySize": 2048,
        "HashAlgorithm": "SHA256"
    },
    "EncryptionConfiguration": {
        "EncryptionLevel": "High",
        "EncryptionAlgorithm": "AES-256",
        "KeyManagement": "Windows",
        "EncryptionEnabled": true,
        "DataAtRestEncryption": true,
        "DataInTransitEncryption": true
    },
    "FirewallConfiguration": {
        "SecurityLevel": "High",
        "DefaultAction": "Deny",
        "AllowClusterTraffic": true,
        "AllowManagementTraffic": true,
        "AllowBackupTraffic": true,
        "AllowMonitoringTraffic": true,
        "LogBlockedConnections": true,
        "LogAllowedConnections": false
    },
    "ServiceAccountConfiguration": {
        "SecurityLevel": "High",
        "UseManagedServiceAccounts": true,
        "PasswordComplexity": "High",
        "PasswordExpirationDays": 90,
        "AccountLockoutThreshold": 5,
        "AccountLockoutDuration": 30
    },
    "RegistrySecurity": {
        "SecurityLevel": "High",
        "RestrictRemoteAccess": true,
        "EnableAuditLogging": true,
        "RestrictAdministrativeAccess": true,
        "EnableSecurityLogging": true
    },
    "FileSystemSecurity": {
        "SecurityLevel": "High",
        "EnableNTFSSecurity": true,
        "RestrictFileAccess": true,
        "EnableAuditLogging": true,
        "RestrictAdministrativeAccess": true
    },
    "EventLogSecurity": {
        "SecurityLevel": "High",
        "EnableSecurityLogging": true,
        "LogRetentionDays": 365,
        "LogRotation": true,
        "CompressionEnabled": true,
        "EncryptionEnabled": true
    },
    "BackupSecurity": {
        "SecurityLevel": "High",
        "EncryptionEnabled": true,
        "CompressionEnabled": true,
        "AccessControl": "Restricted",
        "AuditLogging": true,
        "RetentionDays": 365
    },
    "MonitoringSecurity": {
        "SecurityLevel": "High",
        "EncryptionEnabled": true,
        "AccessControl": "Restricted",
        "AuditLogging": true,
        "DataRetentionDays": 90
    },
    "DisasterRecoverySecurity": {
        "SecurityLevel": "High",
        "EncryptionEnabled": true,
        "AccessControl": "Restricted",
        "AuditLogging": true,
        "DataRetentionDays": 365
    }
}
